include std.io


long rand()
	return 4 # chosen by fair dice roll
	         # guaranteed to be random
end


int main()
	long num = rand
	writeln "Guess a number between 1 and 10\n"
	while 1
		byte[] s = readln
		long n = 0
		for c in s
			if c == '\n'
				break
			end
			n *= 10
			n += c - '0'
		end
		writeln_num n
		if n == num
			writeln "Yay! You found it!\n"
			break
		else
			if n > num
				writeln "Lower\n"
			else
				writeln "Higher\n"
			end
		end
	end
	return 0
end

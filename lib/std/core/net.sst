int connect(byte* ip, ushort port)
	int fd
	__asm
		r1 = ip, r2 = port
		fd = r0
		set	r0,3
		syscall
	end
	return fd
end

int listen(byte* ip, ushort port)
	__asm
		r1 = ip, r2 = port
		fd = r0
		set	r0,4
		syscall
	end
	return fd
end

int accept(int fd)
	__asm
		r1 = fd
		fd = r0
		set	r0,1
		syscall
	end
	return fd
end
